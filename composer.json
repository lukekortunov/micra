{
    "name": "lukekortunov/micra",
    "description": "Skeleton for incredibly simple PHP apps with Routing, DI and Dotenv",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luke Kortunov",
            "email": "luke@kortunov.me"
        }
    ],
    "require": {
        "php": ">=8.1",
        "league/route": "^5.1",
        "laminas/laminas-diactoros": "^2.24",
        "league/container": "^4.2",
        "laminas/laminas-httphandlerrunner": "^2.5",
        "symfony/dotenv": "^6.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "phpunit/phpunit": "^10.0"
    }
}
